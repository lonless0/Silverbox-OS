{
  "arch": "x86",
  "cpu": "pentium4",
  "llvm-target": "i686-pc-unknown-elf",
  "target-endian": "little",
  "target-pointer-width": "32",
  "os": "unknown",
  "data-layout": "e-p:32:32:32-i1:8:8-i8:8:8-i16:16:16-i32:32:32-f32:32:32-f64:64:64-v64:64:64-v128:128:128-a:0:32-f80:128:128-n8:16:32-S32-m:e",
  "pre-link-args": {
    "gcc": [
      "-ffreestanding",
      "-nostdlib",
      "-Wl,--script=link.ld"
    ]
  },

  "features": "-sse3,-ssse3,-sse4.1,-sse4.2,-3dnowa,-avx,-avx2",
  "disable-redzone": true,
  "linker-is-gnu": true,
  "archive-format": "gnu",
  "executables": true,
  "linker": "/usr/local/i686-elf/bin/i686-elf-gcc",
  "eh-frame-header": false,
  "exe-suffix": ".exe"
}
